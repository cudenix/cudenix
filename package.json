			"types": "./dist/ecosystem/client/index.d.ts"
		},
		"./cluster": {
			"import": "./dist/ecosystem/plugins/cluster/index.js",
			"types": "./dist/ecosystem/plugins/cluster/index.d.ts"
		},
		"./compress": {
			"import": "./dist/ecosystem/modules/compress/index.js",
			"types": "./dist/ecosystem/modules/compress/index.d.ts"
		},
		"./cors": {
			"import": "./dist/ecosystem/modules/cors/index.js",
			"types": "./dist/ecosystem/modules/cors/index.d.ts"
		},
		"./events": {
			"import": "./dist/ecosystem/plugins/events/index.js",
			"types": "./dist/ecosystem/plugins/events/index.d.ts"
		},
		"./global-request-context": {
			"import": "./dist/ecosystem/modules/global-request-context/index.js",
			"types": "./dist/ecosystem/modules/global-request-context/index.d.ts"
		},
		"./i18n": {
			"import": "./dist/ecosystem/plugins/i18n/index.js",
			"types": "./dist/ecosystem/plugins/i18n/index.d.ts"
		},
		"./openapi": {
			"import": "./dist/ecosystem/plugins/openapi/index.js",
			"types": "./dist/ecosystem/plugins/openapi/index.d.ts"
		},
		"./package.json": "./package.json",
		"./trycatch": {
			"import": "./dist/ecosystem/modules/trycatch/index.js",
			"types": "./dist/ecosystem/modules/trycatch/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"homepage": "https://github.com/cudenix/cudenix",
	"keywords": [
		"api",
		"bun",
		"framework",
		"http",
		"server",
		"sse",
		"typescript",
		"web",
		"websocket"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"name": "@cudenix/cudenix",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cudenix/cudenix.git"
	},
	"scripts": {
		"build": "bun format && bun lint && rm -rf dist && tsc && tsc-alias",
		"format": "biome format --fix",
		"lint": "biome lint --diagnostic-level=error"
	},
	"sideEffects": false,
	"type": "module",
	"types": "dist/index.d.ts",
	"version": "0.0.48"
}
