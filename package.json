	},
	"description": "cudenix",
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/bun": "1.3.0",
		"tsc-alias": "1.8.16",
		"typescript": "5.9.3"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./client": {
